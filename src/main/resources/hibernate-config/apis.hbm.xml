<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="py.icshare.authorization.APIToAuthorize" table="apis" >
        <id name="apiName" column="name" type="string" />
        <property name="category" column="category" type="string" />
        <property name="chineseText" column="chineseText" type="string" />
        <property name="englishText" column="englishText" type="string" />
    </class>
</hibernate-mapping>