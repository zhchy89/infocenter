<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="py.icshare.iscsiAccessRule.IscsiAccessRuleInformation" table="iscsi_access_rules">
        <id name="ruleId" column="rule_id" />
        <property name="ruleNotes" column="rule_notes" type="string" />
        <property name="initiatorName" column="initiatorname" type="string" />
        <property name="user" column="username" type="string" />
        <property name="passed" column="passed" type="string" />
        <property name="outUser" column="out_user" type="string" />
        <property name="outPassed" column="out_passed" type="string" />
        <property name="permission" column="permission" type="integer" />
        <property name="status" column="status" type="string" />
    </class>
</hibernate-mapping>