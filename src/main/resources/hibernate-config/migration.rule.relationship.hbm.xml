<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="py.icshare.qos.MigrationRuleRelationshipInformation" table="migration_relationship" >
        <id name="relationshipId" column="relationship_id" />
        <property name="storagePoolId" column="storage_pool_id" type="long" />
        <property name="ruleId" column="rule_id" type="long" />
        <property name="status" column="status" type="string" />
    </class>
</hibernate-mapping>
